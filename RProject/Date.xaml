<Window x:Class="RProject.Date"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RProject"
        mc:Ignorable="d"
        Title="" Height="203" Width="187" Background="{StaticResource LightBackground}">
    <Grid Background="{x:Null}">
        <Calendar x:Name="c1" FirstDayOfWeek="Sunday" BorderBrush="Transparent" HorizontalAlignment="Left" VerticalAlignment="Top" SelectionMode="SingleRange" SelectedDatesChanged="User_Click" Margin="0,1,0,0" Background="{x:Null}" Height="165"/>
    </Grid>
</Window>
